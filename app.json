{
  "expo": {
    "name": "myappexpo",
    "slug": "myappexpo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.jpeg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gebrecherkos.myappexpo",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.jpeg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gebrecherkos.myappexpo"
    },
    "web": {
      "favicon": "./assets/icon.jpeg"
    },
    "extra": {
      "eas": {
        "projectId": "81a674d0-b548-44c9-addd-fced45646b21"
      }
    },
    "owner": "gebrecherkos",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/81a674d0-b548-44c9-addd-fced45646b21"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "googleServicesFile": "./google-services.json"
          }
        }
      ]
    ]
  }
}
